@model IEnumerable<MTC.FSP.Web.Models.InspectionType>

@{
    ViewBag.Title = "Inspection Types";
}

<h2>Inspection Types</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Name
        </th>
        <th>
            Code
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.InspectionType1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.InspectionTypeCode)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.InspectionTypeID }) |         
            @Html.ActionLink("Delete", "Delete", new { id=item.InspectionTypeID })
        </td>
    </tr>
}

</table>
