    <script>
        $(function () {
            var d = new Date();
            $('.footable').footable();
            $('#Date').datepicker({
                "setDate": new Date()
            });
            $('#Date').val(d.toLocaleDateString());
            $('#instructions').hide();
            $('#btnInstr').click(function () {
                if ($('#instructions').is(":visible")) {
                    $('#instructions').hide('slideUp');
                } else {
                    $('#instructions').show('slideDown');
                }
            });
        });
    </script>

